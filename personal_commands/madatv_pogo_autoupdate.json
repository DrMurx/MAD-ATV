{
  "ATV - PoGo auto-update: enable": [
    {
      "TYPE": "jobType.PASSTHROUGH",
      "SYNTAX": "rm /sdcard/disableautopogoupdate && exit 0"
    }
  ],
  "ATV - PoGo auto-update: disable": [
    {
      "TYPE": "jobType.PASSTHROUGH",
      "SYNTAX": "touch /sdcard/disableautopogoupdate"
    }
  ],
  "ATV - PoGo auto-update: status": [
    {
      "TYPE": "jobType.PASSTHROUGH",
      "SYNTAX": "[ -f /sdcard/disableautopogoupdate ] && echo enabled || echo disabled"
    }
  ]
}
